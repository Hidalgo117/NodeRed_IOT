    {
        "id": "14d3a4a78fa3b629",
        "type": "function",
        "z": "b91b9fbfdb312e26",
        "g": "1a3333ad4025b3fd",
        "name": "convertir voltaje a %",
        "func": "var valor = msg.payload;\n\n// Verificamos si el valor est√° dentro del rango 1-800\nif (valor > -1 && valor < 1400) {\n    const escalaOriginalMin = 1;\n    const escalaOriginalMax = 1500;\n    const escalaNuevaMin = 1;\n    const escalaNuevaMax = 100;\n\n    // Realizamos la conversi√≥n\n    var valorConvertido = ((valor - escalaOriginalMin) / (escalaOriginalMax - escalaOriginalMin)) * (escalaNuevaMax - escalaNuevaMin) + escalaNuevaMin;\n\n    // Convertimos el valor a un entero (redondeado)\n    msg.payload = Math.round(valorConvertido);\n} else {\n    // Si el valor no est√° en el rango, asignamos el valor 100\n    msg.payload = 100;\n}\n\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
